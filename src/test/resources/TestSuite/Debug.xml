<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Debug Test Suite" verbose="1">
    <test name="Exception Test 1">
        <parameter name="Browser" value="Chrome"></parameter>
        <classes>
            <class name="com.practicetestautomation.tests.exception.TestException"/>
        </classes>
    </test>
    <test name="Wait">
        <parameter name="Browser" value="Chrome"></parameter>
        <classes>
            <class name="com.practicetestautomation.tests.exception.TestExplicit">
                <methods>
                    <include name="TestTimeOutException"></include>
                </methods>
            </class>
        </classes>
    </test>
</suite>
